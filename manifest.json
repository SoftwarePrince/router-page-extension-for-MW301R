{
  "name": "Delete Videos Titles",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "permissions": ["storage", "declarativeNetRequest", "management"],
  "browser_specific_settings": {
    "gecko": {
      "id": "Delete Videos Titles@softwareprince.com",
      "strict_min_version": "53.0"
    }
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules1.json"
      }
    ]
  },
  "content_scripts": [
    {
      "js": ["App.js"],
      "matches": ["*://*.xvideos.com/*"],
      "run_at": "document_end"
    },
    {
      "js": ["close.js"],
      "matches": [
        "*://lookmovie.io/images/logo1.png",
        "https://shopshopextension.com/*",
        "https://hidelen.com/*",
        "https://dvitohrrmq.xyz/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["hot-reload.js", "bg.js"]
  }
}
