{
  "name": "Delete Videos Titles",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "tabs",
    "<all_urls>",
    "management"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "Delete Videos Titles@softwareprince.com",
      "strict_min_version": "53.0"
    }
  },
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": ["*://*.xvideos.com/*"],
      "run_at": "document_end"
    },
    {
      "js": ["MW301R.js"],
      "matches": ["http://192.168.10.1/"],
      "run_at": "document_end"
    },
    {
      "js": ["tastedive.js"],
      "matches": ["https://tastedive.com/movies/like/*"],
      "run_at": "document_end"
    },
    {
      "js": ["close.js"],
      "matches": [
        "*://lookmovie.io/images/logo1.png",
        "https://shopshopextension.com/*",
        "https://hidelen.com/*",
        "https://dvitohrrmq.xyz/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["hot-reload.js", "bg.js"]
  }
}
